# # Program to find first 20 non even prime natural number

# In[3]:


n = int(input("prime numbers u want to print: "))
no = range(1, n*10)
primeno = []
for num in no:
    if num > 2 and len(primeno) <= n-1:
        for i in range(2, num):
            if (num % i) == 0:
                break
        else:
            primeno.append(num)
print("\nHere are the first", n,  " non-even prime numbers: \n")
for x in primeno:
    print(x, end = " ")


